
Program:
#include <stdio.h>
#include <math.h>
int main() {
int x1, y1, x2, y2;
double distance;
printf("Enter x1 and y1: ");
scanf("%d %d", &x1, &y1);
printf("Enter x2 and y2: ");
scanf("%d %d", &x2, &y2);
distance = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
printf("The Euclidean distance between (%d, %d) and (%d, %d) is %lf\n", x1, y1, x2, y2, distance);
return 0;
}
